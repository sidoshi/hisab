{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "sql:default",
    "sql:allow-execute",
    "store:default",
    "core:webview:allow-print",
    "dialog:default",
    "fs:default",
    "fs:allow-app-write",
    "core:window:allow-destroy",
    "process:default"
  ]
}